{
	"name": "phpcocktail/cocktail",
	"version": "1.1",
	"dist": {
		"url": "https://github.com/phpcocktail/cocktail.git",
		"type": "vcs"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "phpcocktail/block",
				"version": "1.1",
				"source": {
					"url": "https://github.com/phpcocktail/block.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"classmap": ["src/"],
					"files": ["bootstrap.php"]
				}
			}
		}
	],
	"require": {
		"electrolinux/phpquery": "dev-master",
		"phpcocktail/block": "*"
	},
	"autoload": {
		"classmap": [
			"src/"
		],
		"files": [
			"bootstrap.php"
		]
	}
}
